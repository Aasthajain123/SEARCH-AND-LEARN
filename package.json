	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"concurrently": "^3.6.1",
		"express": "^4.16.4",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.7.5",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"quickemailverification": "^1.0.2"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
